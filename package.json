  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "__predev": "npm run generate-database-types",
    "prestart:debug": "npm run generate-database-types",
    "dev": "dotenv -e .env/.env.local nest start --swc",
    "dev-bun": "bun --env-file=.env/.env.local ./node_modules/@nestjs/cli/bin/nest.js start",
    "start:debug": "dotenv -e .env/.env.local nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome lint",
    "lint:fix": "biome lint --write --unsafe",
    "format": "biome format --write",
    "test": "vitest run",
    "test:ci": "vitest run --maxWorkers=1 --no-color",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:e2e": "vitest run --config ./vitest-e2e.config.ts",
    "install-pre-commit-hook": "lefthook install",
    "generate-database-types": "dotenv -e ./.env/.env.local graphql-codegen -- --config codegen-hetarchief.yml",
    "bump": "bump-package-versions --part=patch --strategy=highest ./package.json"
  },
