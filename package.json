    "@nestjs/swagger": "5.2.0",
    "@studiohyperdrive/pagination": "1.0.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "connect-redis": "6.0.0",
    "date-fns": "2.28.0",
    "date-fns-tz": "1.3.0",
    "express-session": "1.17.2",
    "fs-extra": "10.0.0",
    "got": "11.8.3",
    "helmet": "5.0.2",
    "i18next": "21.6.3",
    "joi": "17.5.0",
    "lodash": "4.17.21",
    "query-string": "7.1.0",
    "redis": "3.1.2",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "7.4.0",
    "saml2-js": "3.0.1",
    "session-file-store": "1.5.0",
    "swagger-ui-express": "4.3.0",
    "xml-js": "1.6.11"
